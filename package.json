    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "react": "^16.8.4",
    "react-dom": "^16.10.2",
    "source-sans-pro": "^3.6.0",
    "styled-components": "^4.4.0"
  }
}
